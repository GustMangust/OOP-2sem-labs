<UserControl x:Class="Lab6_7.CustomElement"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Lab6_7"
             xmlns:vsc="clr-namespace:Lab6_7;assembly=Lab6-7"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="400">
    <Grid>
        <StackPanel Background="Blue">
            <StackPanel x:Name="spText" Margin="70">
                <TextBlock FontSize="30" Foreground="White">Custom Element</TextBlock>
            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
